    },
    {
      "name": "monster-templates",
      "label": "Monster Templates",
      "path": "./packs/monster-templates.db",
      "type": "Item"
    },
    {
      "name": "sample-macros",
      "label": "Sample Macros",
      "path": "./packs/sample-macros.db",
      "type": "Macro"
    },
    {
      "name": "roll-tables",
      "label": "Roll Tables",
      "path": "./packs/roll-tables.db",
      "type": "RollTable"
    },
    {
      "name": "ultimate-equipment",
      "label": "Ultimate Equipment",
      "path": "./packs/ultimate-equipment.db",
      "type": "RollTable"
    },
    {
      "name": "pf1e-rules",
      "label": "Pathfinder 1e Rules",
      "path": "./packs/pf1e-rules.db",
      "type": "JournalEntry"
    }
  ],
  "languages": [
    { "lang": "en", "name": "English", "path": "lang/en.json" },
    { "lang": "de", "name": "Deutsch (German)", "path": "lang/de.json" },
    { "lang": "fr", "name": "Française (French)", "path": "lang/fr.json" },
    { "lang": "it", "name": "Italiano (Italian)", "path": "lang/it.json" },
    { "lang": "es", "name": "Español (Spanish)", "path": "lang/es.json" },
    { "lang": "cn", "name": "Chinese", "path": "lang/cn.json" }
