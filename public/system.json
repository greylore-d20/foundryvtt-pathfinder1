    {
      "name": "weapons-and-ammo",
      "label": "Weapons and Ammo",
      "path": "./packs/weapons-and-ammo.db",
      "type": "Item"
    },
    { "name": "racialhd", "label": "Racial HD", "path": "./packs/racial-hd.db", "type": "Item" },
    { "name": "races", "label": "Races", "path": "./packs/races.db", "type": "Item" },
    {
      "name": "class-abilities",
      "label": "Class Abilities",
      "path": "./packs/class-abilities.db",
